{
  "name": "photogen",
  "version": "0.0.10",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "Flavian DESVERNE <desverne@prisma.io>",
  "files": [
    "dist",
    "photogen_ncc_build"
  ],
  "bin": {
    "photogen": "dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && rm -rf photogen_ncc_build && tsc && ncc build -e nexus-tmp-chainable-method src/photogen/index.ts -o photogen_ncc_build",
    "prepublish": "yarn build",
    "prettify": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "nexus-tmp-chainable-method": "^0.0.2"
  },
  "devDependencies": {
    "@prisma/cli": "^0.0.10",
    "@types/node": "^12.0.8",
    "@zeit/ncc": "^0.18.5",
    "prettier": "^1.17.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true
  }
}
